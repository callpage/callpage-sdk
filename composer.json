{
    "name": "callpage/callpage-sdk",
    "description": "PHP wrapper for CallPage REST API",
    "keywords": [
        "CallPage",
        "callpage-sdk"
    ],
    "homepage": "https://github.com/callpage/callpage-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "CallPage Team",
            "email": "dev@callpage.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4"
    },
    "require-dev": {
        "pestphp/pest": "^4.0",
        "laravel/pint": "^1.0",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "CallPage\\CallPage\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CallPage\\CallPage\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
            "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
